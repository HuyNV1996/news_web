<script>
	var isScrollingUp = false;
	var lastScrollY = window.scrollY;

	window.addEventListener('scroll', function () {
		var currentScrollY = window.scrollY;

		if (currentScrollY < lastScrollY) {
			isScrollingUp = true;
		} else {
			isScrollingUp = false;
		}

		lastScrollY = currentScrollY;

		if (isScrollingUp) {
			var diachi = document.getElementById('diachii');
			var lienhe = document.getElementById('lienhee');
			var tuyendung = document.getElementById('tuyendungg');

			diachi.style.height = '60px';
			tuyendung.style.height = '40px';
			lienhe.style.height = '36px';
		} else {
			diachi.style.height = '1px';
			lienhe.style.height = '1px';
			tuyendung.style.height = '1px';
		}
	});


</script>