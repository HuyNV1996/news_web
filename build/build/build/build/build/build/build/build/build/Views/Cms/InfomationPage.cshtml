@using web_ttkd.Models.Regions;
@using web_ttkd.Models;
@model web_ttkd.Models.InfomationPage;

@{
	var jobList = ViewBag.ListJobs;
	var HNConsultant = Model.Consultancy.Where(x => x.Regions.EnumValue.Equals("HaNoi")).ToList();
